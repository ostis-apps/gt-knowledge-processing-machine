proc_find_weighted_graph
=> nrel_main_idtf:
	[Программа проверки графа на взвешеность] (* <- lang_ru;; *);

=> nrel_initiation_condition_and_result: (..scp_program_proc_find_weighted_graph_initiation_condition
					  => ..scp_program_proc_find_weighted_graph_result);

-> rrel_key_sc_element: concept_graph; concept_planar_graph;
-> rrel_main_key_sc_element: concept_planar_graph;

<- rrel_key_sc_element:
	..scp_program_proc_find_weighted_graph_behavior
	(*
		<- sc_description_of_scp_program_behavior;;
		<= nrel_sc_text_translation:
		...
		(*
			-> rrel_example:
				[Это программа создает из понятия "взвешеный граф" к графу либо дугу приндлежности, если граф взвешеный, либо отрицательную дугу, если граф невзвешеный];;
		*);;
	*);;

..scp_program_proc_find_weighted_graph_initiation_condition = [*
	concept_graph _-> _curr_graph;;
*];;

..scp_program_proc_find_weighted_graph_result
	<- nrel_disjunction;
	-> ..scp_program_proc_find_weighted_graph_result1;
	-> ..scp_program_proc_find_weighted_graph_result2;;

..scp_program_proc_find_weighted_graph_result1
= [*
	concept_weighted_graph _-> _curr_graph;;
*];;

..scp_program_proc_find_weighted_graph_result2
= [*
	concept_weighted_graph _-|> _curr_graph;;
*];;
