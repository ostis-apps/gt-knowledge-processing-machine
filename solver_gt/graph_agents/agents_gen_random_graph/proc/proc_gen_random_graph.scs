                _-> rrel_5:: rrel_fixed:: rrel_scp_var:: rrel_struct:: _result;;

                _=> nrel_goto:: .._operator_take_vertex_from_set2;;
        *);;

        _-> .._return_operator (*
                _<- return;;
        *);;

*);;

*];;