                _-> rrel_2:: rrel_assign:: rrel_scp_var:: _arc_to_new_node;;
                _-> rrel_3:: rrel_assign:: rrel_scp_var:: _node_in_set1;;

                _=> nrel_then:: .._operator_take_vertex_from_set2;;
